{
  "name": "snapdragon-is-inside",
  "description": "Snapdragon plugin that adds an `isInside` method to the compiler instance for checking if a node is currently inside a given node type.",
  "version": "0.1.1",
  "homepage": "https://github.com/jonschlinkert/snapdragon-is-inside",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/snapdragon-is-inside",
  "bugs": {
    "url": "https://github.com/jonschlinkert/snapdragon-is-inside/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "define-property": "^0.2.5",
    "snapdragon-util": "^0.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "generategenerator",
    "inside",
    "is",
    "snapdragon"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "snapdragon",
        "snapdragon-util",
        "snapdragon-capture",
        "snapdragon-node",
        "snapdragon-position"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
